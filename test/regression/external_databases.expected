$ generatetree "prefix"

# create in-source-tree user db files and fill in tables

#########################################
$ sqlite3 "prefix/faction1.db" "CREATE TABLE user_table(name TEXT, external_data TEXT);"

$ sqlite3 "prefix/faction1.db" "INSERT INTO user_table VALUES ('old_file', 'faction1 old file data');"

$ sqlite3 "prefix/faction1.db" "INSERT INTO user_table VALUES ('1MB', 'faction1 1MB file');"

$ sqlite3 "prefix/faction1.db" "INSERT INTO user_table VALUES ('repeat_name', 'faction1 repeat_name');"

#########################################
$ sqlite3 "prefix/directory/faction1.db" "CREATE TABLE user_table(name TEXT, external_data TEXT);"

$ sqlite3 "prefix/directory/faction1.db" "INSERT INTO user_table VALUES ('directory', 'faction1 directory');"

$ sqlite3 "prefix/directory/faction1.db" "INSERT INTO user_table VALUES ('executable', 'faction1 executable file');"

#########################################
$ sqlite3 "prefix/leaf_directory/faction1.db" "CREATE TABLE user_table(name TEXT, external_data TEXT);"

$ sqlite3 "prefix/leaf_directory/faction1.db" "INSERT INTO user_table VALUES ('no match', 'this should not show up in the index');"

#########################################

#########################################
$ sqlite3 "prefix/faction2.db" "CREATE TABLE user_table(type TEXT, external_data TEXT);"

$ sqlite3 "prefix/faction2.db" "INSERT INTO user_table VALUES ('d', 'faction2 root directory');"

$ sqlite3 "prefix/faction2.db" "INSERT INTO user_table VALUES ('f', 'faction2 file type data in root');"

#########################################
$ sqlite3 "prefix/directory/subdirectory/faction2.db" "CREATE TABLE user_table(type TEXT, external_data TEXT);"

$ sqlite3 "prefix/directory/subdirectory/faction2.db" "INSERT INTO user_table VALUES ('d', 'faction2 subdirectory');"

