[('name', '1KB')]
[('name', '1MB')]
[('name', 'directory_symlink')]
[('name', 'executable')]
[('name', 'file_symlink')]
[('name', 'leaf_file1')]
[('name', 'leaf_file2')]
[('name', 'old_file')]
[('name', 'readonly')]
[('name', 'repeat_name')]
[('name', 'repeat_name')]
[('name', 'unusual, name?#')]
[('name', 'writable')]

